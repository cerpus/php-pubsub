{
    "name": "cerpus/pubsub",
    "description": "Client for RabbitMQ pub-sub",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Max Moeschinger",
            "email": "max.moeschinger@cerpus.com"
        },
        {
            "name": "Emma Cecilie Hughes",
            "email": "emma.hughes@cerpus.com"
        }
    ],
    "require": {
        "php": "8.0.*|8.1.*|8.2.*|8.3.*",
        "php-amqplib/php-amqplib": "^3.1",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Cerpus\\PubSub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerpus\\PubSub\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
